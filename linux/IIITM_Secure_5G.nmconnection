[connection]
id=IIITM_Secure_5G
uuid=e7f5b3d1-9b2b-5c0b-0b0b-0b0b0b0b0b0b
type=wifi
interface-name=wlan0

[wifi]
mode=infrastructure
ssid=IIITM_Secure_5G

[wifi-security]
key-mgmt=wpa-eap

[802-1x]
eap=ttls;
identity=USER_LDAP_ID
password=USER_LDAP_PASSWORD
phase2-auth=pap
ca-cert=/usr/share/ca-certificates/iiitm/IIITM-CA.crt
domain-suffix-match=iiitm.ac.in

[ipv4]
method=auto

[ipv6]
addr-gen-mode=stable-privacy
method=auto
