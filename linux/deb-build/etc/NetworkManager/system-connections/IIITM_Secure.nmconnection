[connection]
id=IIITM_Secure
uuid=d6e4a2c0-8a1a-4b9a-9a9a-9a9a9a9a9a9a
type=wifi
interface-name=wlan0

[wifi]
mode=infrastructure
ssid=IIITM_Secure

[wifi-security]
key-mgmt=wpa-eap

[802-1x]
eap=ttls;
identity=USER_LDAP_ID
password=USER_LDAP_PASSWORD
phase2-auth=pap
ca-cert=/usr/share/ca-certificates/iiitm/IIITM-CA.crt
domain-suffix-match=iiitm.ac.in

[ipv4]
method=auto

[ipv6]
addr-gen-mode=stable-privacy
method=auto
